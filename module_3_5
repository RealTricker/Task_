def get_multiplied_digits(number):
    
    str_number = str(number)
    first = int(str_number[0])

    if len(str_number) > 1:
        return first * get_multiplied_digits(int(str_number[1:]))
    else:
        return first

result = get_multiplied_digits(40203)
print(result)  

result2 = get_multiplied_digits(402030)
print(result2)  

result3 = get_multiplied_digits(12345)
print(result3) 

result4 = get_multiplied_digits(0)
print(result4) 

result5 = get_multiplied_digits(100)
print(result5) 

result6 = get_multiplied_digits(100000)
print(result6) 

result7 = get_multiplied_digits(10)
print(result7) 

result8 = get_multiplied_digits(5)
print(result8) 
